{
  "group_name": "example",
  "project": "energy-efficiency",
  "version": "1",
  "data_config": {
    "source": "csv",
    "local_name_source": "data/ENB2012_data.csv",
    "separation" : {
        "kind": "random", 
        "random_state": 1
    },
    "data": {
        "targetslices": [{"column": "Glazing Area Distribution", "type": "numerical", "slices": 5}]
    } 
  },
  "models_configs": [
    {
      "name": "EEI",
      "column_target": "EEI",
      "wrapper": "catboost",
      "relative_features": [],
      "features": [        
        {
          "name": "Wall_Roof_Ratio",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        },
        {
          "name": "Compactness_Height",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        },
        {
          "name": "Glazing Area Distribution",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        },
        {
          "name": "Orientation",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"},
          "encoding" : "WoE_cat_to_num"
        }
      ]
    }
  ]
}

